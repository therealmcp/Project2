    <div class="bg">

    <nav class="navbar navbar-expand-lg navbar-light bg-dark">
        <a class="navbar-brand" id="brandish" href="#">APP NAME</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
          <div class="navbar-nav">
            <a class="nav-item nav-link active" href="/">Home <span class="sr-only">(current)</span></a>
            <a class="nav-item nav-link" href="/message">View Posts</a>
            <a class="nav-item nav-link" href="/post">Make Post</a>
             <a class="nav-item nav-link" href="/api/logout">Logout</a>
          </div>
        </div>
      </nav>  

      <div class="row">

      <div class="card text-center" style="width: 18rem;" id="topCard">
        <img class="card-img-top" src="images/{{userData.pic}}" alt="Card image cap">
        <div class="card-body">
            <!-- User Name here in  Card Title -->
          <h5 class="card-title">{{userData.name}}</h5>
            <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
          <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
          <span class="badge badge-primary"><img class="card-img-top" src="images/{{userData.badge}}" alt="Card imagers"></span>
          <hr>
          <h5 class="card-title">{{userData.bio}}</h5>
          <h2 class="card-text">{{userData.games}}</h2>
          <h2 class="card-text">{{userData.birthdate}}</h2>
          <h2 class="card-text">{{userData.email}}</h2>
        </div>
      </div>

  

          <div class="col-sm-6" id="messages">
          <div class="card">
            <h5 class="card-header">User Name Here</h5>
            <div class="card-body">
              <!-- <h5 class="card-title">Special title treatment</h5> -->
              <p class="card-text">Here is the users Post.</p>
              {{#each posts}}
                <p>{{this.dataValues.post}}</p>
              {{/each}}
              <p class="card-text">Games are fun and all kinds of other noise.</p>
              <p class="card-text">Talk talk talk talk.</p>
            
              <button type="button" class="btn btn-outline-primary">Add Comment</button>
              <hr>
              <br>
              <div class="col-sm-6">
                <div class="card" id="commentCard">
                  <div class="card-body">
                    Comments go in this card.
                  </div>
                </div>
                </div>
            </div>
          </div>
          </div>
</div>

</div>
